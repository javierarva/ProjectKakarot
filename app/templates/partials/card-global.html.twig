<div onclick="window.location.href='{{ path('app_quiz_view', { 'id': quiz.id }) }}'" class="cardQuiz">
    <div class="oneQuiz">
        <div class="card-body">
            <div class="category">
                <img src="/img/views/{{ quiz.image }}" class="imgcard" alt="Imagen de {{ quiz.name }}">
                <h2 class="card-title">{{ quiz.name }}</h2>
            </div>
            <div class="status">
                <p>Estado: {{ quiz.status }}</p>
                <svg alt="Ir al quiz de {{ quiz.name }}" id="icon" class="svgAvatar" xmlns="http://www.w3.org/2000/svg" height="48" width="48"><path d="M15.2 43.9 12.4 41.05 29.55 23.9 12.4 6.75 15.2 3.9 35.2 23.9Z"/></svg>
            </div>
        </div>
    </div>
</div>